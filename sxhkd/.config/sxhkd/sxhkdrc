# Basic binds
super + Return
	$TERMINAL
super + d
	dmenu_run
super + r
	$TERMINAL -e $FILE
super + g
  ~/.local/bin/google-search.sh
super + shift + a
	$TERMINAL -e alsamixer; pkill -RTMIN+10 blocks
super + w
	$TERMINAL -e sudo nmtui
super + shift + w
	$BROWSER
super + shift + s
  ~/.local/bin/xps-display-only-layout.sh
super + shift + d
  ~/.local/bin/dual-display-layout.sh
super + Escape
	pkill -USR1 -x sxhkd
