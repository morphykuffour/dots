	"keyMappings": [
		"#!no-check",
		"# Move current tab to new window ",
		"map W moveTabToNewWindow",
		"# Make a current tab zoom in ",
		"map z zoomIn",
		"",
		"# Make a current tab zoom out ",
		"map Z zoomOut",
		"",
		"# Make a current tab zoom reset to 100%",
		"map = zoomReset",
		"",
		"map f LinkHints.activate",
		"",
		"map o Vomnibar.activate",
		"",
		"# open image in new tab",
		"map sx LinkHints.activateOpenImage",
		"",
		"run <v-nw> $s#reuse=\"new-wnd\" mask",
		"run P openCopiedUrlInCurrentTab",
		"",
		"# Linkding integration (via Tailscale)",
		"# Save current page to linkding",
		"map ,l openUrl reuse=1 url=\"http://raspberrypi:9090/bookmarks/new?url=$U&title=$T&auto_close\"",
		"# Save current page with selection as description",
		"map ,L openUrl reuse=1 url=\"http://raspberrypi:9090/bookmarks/new?url=$U&title=$T&description=$S&auto_close\"",
		"",
		"# Search linkding bookmarks",
		"map ,b Vomnibar.activate keyword=\"ld\"",
		"",
